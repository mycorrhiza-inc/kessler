                },
                "docket_id": {
                    "type": "string"
                },
                "file_name": {
                    "type": "string"
                },
                "item_no": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "nypuc_doctype": {
                    "type": "string"
                },
                "organization": {
                    "type": "string"
                },
                "serial": {
                    "type": "string"
                },
                "url": {
                    "type": "string"
                }
            }
        },
        "tasks.ScraperInfoPayload": {
            "type": "object",
