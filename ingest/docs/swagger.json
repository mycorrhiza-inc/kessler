                "file_class": {
                    "type": "string"
                },
                "file_type": {
                    "type": "string"
                },
                "file_url": {
                    "type": "string"
                },
                "internal_source_name": {
                    "type": "string"
                },
                "item_number": {
                    "type": "string"
                },
                "lang": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "published_date": {
                    "type": "string"
                },
                "state": {
                    "type": "string"
                },
                "text": {
                    "type": "string"
                }
            }
        }
    }
